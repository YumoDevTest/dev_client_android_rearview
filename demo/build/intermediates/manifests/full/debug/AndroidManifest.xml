<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mapbar.demo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="22" />

    <!-- 声明权限，接收实时数据广播 -->
    <uses-permission android:name="com.mapbar.android.obd.rearview.permission.REALTIMEDATA" />
    <!-- 声明权限，接收车辆状态广播 -->
    <uses-permission android:name="com.mapbar.android.obd.rearview.permission.CARSTATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.mapbar.demo.External_broadcast_car_statusActivity" >
        </activity>
        <activity android:name="com.mapbar.demo.External_broadcast_realtime2Activity" >
        </activity>
        <activity android:name="com.mapbar.demo.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>